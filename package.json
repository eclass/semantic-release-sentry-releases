{
  "name": "@eclass/semantic-release-sentry-releases",
  "version": "2.4.3",
  "description": "semantic-release plugin to create releases in sentry",
  "main": "src/index.js",
  "scripts": {
    "lint": "prettier-standard --lint '{src,test}/**/*.js'",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "test": "nyc mocha test"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclass/semantic-release-sentry-releases.git"
  },
  "keywords": [
    "release",
    "semantic-release",
    "sentry"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eclass/semantic-release-sentry-releases/issues"
  },
  "homepage": "https://github.com/eclass/semantic-release-sentry-releases#readme",
  "dependencies": {
    "@semantic-release/error": "^2.1.0",
    "@sentry/cli": "1.54.0",
    "aggregate-error": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@eclass/eslint-config": "1.3.1",
    "@eclass/semantic-release-npm-github-config": "3.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@sentry/types": "5.17.0",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/mock-require": "2.0.0",
    "@types/node": "12.12.47",
    "@types/semantic-release": "17.0.1",
    "chai": "4.2.0",
    "eslint": "7.3.0",
    "eslint-plugin-array-func": "3.1.6",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsdoc": "25.4.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mocha": "7.2.0",
    "mock-require": "3.0.3",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "16.3.0",
    "semantic-release": "17.1.0",
    "temp-write": "4.0.0",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "semantic-release": ">=11.0.0 <18.0.0"
  },
  "eslintConfig": {
    "extends": "@eclass"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier-standard"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ]
  },
  "release": {
    "extends": "@eclass/semantic-release-npm-github-config"
  },
  "nyc": {
    "extends": "nyc-config-common",
    "branches": 70,
    "lines": 80,
    "functions": 100,
    "statements": 80
  }
}
